{
    "name": "liveintent/telescope-prometheus-exporter",
    "description": "Prometheus exporter for metrics gathered by telescope.",
    "keywords": [
        "liveintent",
        "laravel",
        "telescope",
        "prometheus",
        "exporter"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "laravel/telescope": "^4.4",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "LiveIntent\\TelescopePrometheusExporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiveIntent\\TelescopePrometheusExporter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiveIntent\\TelescopePrometheusExporter\\TelescopePrometheusExporterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
